{
  "name": "sourceability/console-toolbar-bundle",
  "type": "library",
  "description": "This bundle enables displaying the web toolbar in the console/terminal.",
  "homepage": "https://github.com/sourceability/ConsoleToolbarBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Adrien Brault",
      "email": "adrien.brault@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1 || ^8.0",
    "symfony/console": "^4.4 || ^5.0",
    "symfony/dom-crawler": "^4.4 || ^5.0",
    "symfony/framework-bundle": "^4.4 || ^5.0",
    "symfony/twig-bundle": "^4.4 || ^5.0",
    "symfony/web-profiler-bundle": "^4.4 || ^5.0"
  },
  "require-dev": {
    "behat/behat": "^3.8",
    "friends-of-behat/symfony-extension": "^2.0",
    "phpstan/phpstan": "^0.12.85",
    "phpstan/phpstan-deprecation-rules": "^0.12.6",
    "phpstan/phpstan-strict-rules": "^0.12.9",
    "phpunit/phpunit": "^8.0 || ^9.0",
    "symplify/easy-coding-standard": "^9.3"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Sourceability\\ConsoleToolbarBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sourceability\\ConsoleToolbarBundle\\Test\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/sourceability/ConsoleToolbarBundle/issues",
    "source": "https://github.com/sourceability/ConsoleToolbarBundle"
  }
}
